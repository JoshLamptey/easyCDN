{
    "manifest_version": 2,
    "name": "easyCDN",
    "version": "1.0",
    "content_scripts":[
      {
       "matches":["<all_urls>"],
       "js":["assets/js/index.js"]
      }
     ],
  
    "description": "A simple browser extension which is one place for all common CDN links. Great Tool for developers.",
    "homepage_url": "https://github.com/RamGoel/easyCDN",
    "icons": {
      "48": "assets/images/icon.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "assets/images/icon.png",
      "default_title": "easyCDN",
      "default_popup": "index.html"
      
    },
  
    "web_accessible_resources": [
      "beasts/frog.jpg",
      "beasts/turtle.jpg",
      "beasts/snake.jpg"
    ]
  
  }
  